{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint-config-airbnb-typescript-prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-restricted-syntax": 0
  }
}
